{
  "version": 2,
  "builds": [
    {
      "src": "api/consulta.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/consulta",
      "dest": "api/consulta.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/api/(.*)", 
      "dest": "/api/$1.js"
  }
}
