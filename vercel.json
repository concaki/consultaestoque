{
  "version": 2,
  "builds": [
    { "src": "api/consulta.js", "use": "@vercel/node" },
    { "src": "api/refresh.js", "use": "@vercel/node" },
    { "src": "api/teste.js", "use": "@vercel/node" },
    { "src": "api/testeFetch.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/consulta", "dest": "api/consulta.js" },
    { "src": "/api/refresh", "dest": "api/refresh.js" },
    { "src": "/api/teste", "dest": "api/teste.js" },
    { "src": "/api/testeFetch", "dest": "api/testeFetch.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "functions": {
    "api/*.js": {
      "includeFiles": [
        "package.json",
        "package-lock.json",
        "node_modules/**"
      ]
    }
  }
}
