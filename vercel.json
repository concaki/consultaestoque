{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/consulta",
      "dest": "api/consulta.js"
    },
    {
      "src": "/api/refresh",
      "dest": "api/refresh.js"
    },
    {
      "src": "/api/login",
      "dest": "api/login.js"
    },
    {
      "src": "/api/callback",
      "dest": "api/callback.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
